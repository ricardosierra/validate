{
    "name": "ricardosierra/validate",
    "type": "library",
    "description": "Validate is a PHP library that validate and filter data for you.",
    "keywords": ["validate", "filter", "data"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra"
        }
    ],
    "require": {
        "php": ">=7.0",
        "fzaninotto/faker": ">=1.8",
        "nesbot/carbon": ">=2.0|1.*",
        "guzzlehttp/guzzle": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "Validate\\": "src/Validate/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Validate\\Test\\": "test/Validate/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
